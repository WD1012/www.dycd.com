{
  "name": "myexpress",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "8.9.1"
  },
  "config": {
    "port": "3013"
  },
  "scripts": {
    "dev-start": "pm2 start ./bin/wwwdev -i 0 --name 'dev.www'",
    "dev-stop": "pm2 stop ./bin/wwwdev",
    "dev-restart": "pm2 restart ./bin/wwwdev",
    "dev-reload": "pm2 reload ./bin/wwwdev",
    "test-start": "pm2 start ./bin/wwwtest -i 0 --name 'test.www'",
    "test-stop": "pm2 stop ./bin/wwwtest",
    "test-restart": "pm2 restart ./bin/wwwtest",
    "test-reload": "pm2 reload ./bin/wwwtest",
    "pro-start": "pm2 start ./bin/wwwpro -i 0 --name 'pro.www'",
    "pro-stop": "pm2 stop ./bin/wwwpro",
    "pro-restart": "pm2 restart ./bin/wwwpro",
    "pro-reload": "pm2 reload ./bin/wwwpro"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~3.1.0",
    "ejs": "~2.5.7",
    "express": "~4.16.2",
    "morgan": "~1.9.0",
    "multer": "^1.3.0",
    "node-fetch": "^1.7.3",
    "request": "^2.83.0",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.18.13",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-babel": "^7.0.0",
    "gulp-cache": "^1.0.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-eslint": "^4.0.0",
    "gulp-filter": "^5.0.1",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.0.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^3.1.0",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-uglify": "^3.0.0",
    "gulp-useref": "^3.1.3",
    "main-bower-files": "^2.13.1",
    "run-sequence": "^2.2.0",
    "wiredep": "^4.0.0"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ]
    }
  }
}
